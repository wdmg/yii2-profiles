{
  "name": "wdmg/yii2-profiles",
  "description": "System of accounting user profiles",
  "keywords": ["yii2", "yii2-profiles", "profiles", "user profiles", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.0.0",
  "homepage": "https://github.com/wdmg/yii2-profiles",
  "support": {
    "source": "https://github.com/wdmg/yii2-profiles",
    "issues": "https://github.com/wdmg/yii2-profiles/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.3.1",
    "wdmg/yii2-users": "^1.2.3",
    "wdmg/yii2-helpers": "^1.4.5",
    "wdmg/yii2-validators": "^1.0.7",
    "wdmg/yii2-selectinput": "^1.0.9"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\profiles\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin",
      "supportLocales": [
        "ru-RU",
        "uk-UA",
        "en-US"
      ],
      "reservedFields": [
        "admin",
        "administrator",
        "root",
        "superuser",
        "supervisor",
        "timezone"
      ]
    }
  }
}